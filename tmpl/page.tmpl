  <title>{{.Title}} - Homin.dev </title>
  <meta name="author" content="Homin Lee">
  <meta name="description" content="Homin.dev web sites">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://homin.dev/asset/css/tachyons.min.css">
  <meta name="color-scheme" content="light dark">
  <style>
    :root {
      --clover-green: #5BE584;
      --clover-dark: #2E7D32;
    }

    @media (prefers-color-scheme: dark) {
      .text-dark-aware {
        color: #f5f5f5 !important;
      }
      .bg-dark-aware {
        background-color: #121212 !important;
      }
      .bg-light-gray {
        background-color: #333 !important;
        color: #f5f5f5;
      }
      .text-clover {
        color: var(--clover-green) !important;
      }
      .text-muted {
        color: #bbb !important;
      }
    }

    @media (prefers-color-scheme: light) {
      .text-dark-aware {
        color: #000 !important;
      }
      .bg-dark-aware {
        background-color: #fff !important;
      }
      .text-clover {
        color: var(--clover-dark) !important;
      }
      .text-muted {
        color: #555 !important;
      }
    }
  </style>
</head>
<body class="tc bg-dark-aware text-dark-aware">
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TDBD9KS9"
