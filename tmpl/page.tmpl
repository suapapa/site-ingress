  <h1 class="f2 lh-title">{{.Title}}</h1>
  <img src="{{.Img}}">

  <p class="f3 fw3 pa3">{{.Msg}}</p>

  <div class="flex flex-wrap justify-center pr4 pl4 pb4 items-start lh-copy">
    {{range .Links}}
    {{if not .Hide}}
    <div class="bg-light-gray w5 mw5 tl ma2 pa2 shadow-3">
      <p class="f3 b pa0 ma0">
        <a href="{{.Link}}" class="link underline-hover text-clover hover-orange">{{.Name}}</a>
      </p>
      <p class="f6 pa0 ma0 text-muted">{{.Desc}}</p>
    </div>
    {{end}}
    {{end}}
  </div>

